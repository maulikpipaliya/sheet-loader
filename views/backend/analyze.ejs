<%- include header.ejs %>

        <%- include sidebar.ejs %>

        <div class="rightpart">

            <div class="log-wrightdashboard">
                <div class="col-lg-12 tophead">
                    <h1>Analyze Your Data </h1>
                    <div class="info">
                        <h4>Welcome <label><%= username %></label></h4>
                        <a href="<%= base_url %>/logout">Logout</a>
                    </div>
                </div>
            </div>

            <div class="bodycontent">
                <div class="col-lg-12 wrapform">

                    <nav aria-label="breadcrumb">
                        <ol class="breadcrumb">
                          <li class="breadcrumb-item" aria-current="page"><a href="<%= base_url %>/dashboard/listingdata/original">Listing Table</a></li>
                          <li class="breadcrumb-item active" aria-current="page">Analyze Data <%= tbname %></li>
                        </ol>
                      </nav>

                    <h3>What do you want to do ?</h3>

                    <form method="POST" action="<%= base_url %>/dashboard/analyzed">

                        <input type="hidden" name="_csrf" value="<%= csurfToken %>">

                        <input type="hidden" name="tbname" value="<%= tbname %>">

                        <label for="ad1"><input type="radio" name="ad" id="ad1" value="1" required=""> <span></span> <label for="ad1">Check duplicate data</label></label>

                        <label for="ad2"><input type="radio" name="ad" id="ad2" value="2" required=""> <span></span> <label for="ad2">Counting by grouping data</label></label>

                        <input type="submit" name="submit" value="SUBMIT">


                    </form>

                </div>
            </div>

        </div>

<%- include footer.ejs %>